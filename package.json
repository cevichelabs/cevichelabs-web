{
  "name": "cevichelabs-web",
  "private": true,
  "description": "A simple starter to get up and developing quickly with next",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "dotenv-flow": "^3.2.0",
    "ftp-deploy": "^2.4.3",
    "next": "^13.3.0",
    "next-seo": "^6.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "lazysizes": "^5.3.2",
    "prettier": "2.8.7",
    "react-test-renderer": "^18.3.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "next"
  ],
  "license": "MIT",
  "scripts": {
    "build": "next build && next export -o build/",
    "clean": "next clean",
    "deploy": "node deploy",
    "dev": "next dev",
    "serve": "node serve",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "local": "yarn build --prefix-paths && yarn serve --prefix-paths",
    "lint": "next lint",
    "start": "next start",
    "sync": "browser-sync start --proxy localhost:8000 --watch ./src",
    "test": "jest"
  }
}